{
    "name": "stubbles/xml",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Classes to work with XML",
    "require": {
        "php": ">=5.6.0",
        "stubbles/core": "^7.0",
        "stubbles/date": "^5.5"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.1",
        "bovigo/callmap": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\xml\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
