{
    "name": "stubbles/xml",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Classes to work with XML",
    "require": {
        "php": "^7.3",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "stubbles/ioc": "^10.0",
        "stubbles/reflect": "^9.1",
        "stubbles/date": "^8.0.1"
    },
    "require-dev": {
        "ext-xsl": "*",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "mikey179/vfsstream": "^1.6.8",
        "phpunit/phpunit": "^9.0",
        "bovigo/callmap": "^6.0",
        "bovigo/assert": "^6.0",
        "stubbles/sequence": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\xml\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\xml\\": "src/test/php",
            "stubbles\\helper\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
