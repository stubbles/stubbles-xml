{
    "name": "stubbles/xml",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Classes to work with XML",
    "require": {
        "php": "^7.0",
        "stubbles/ioc": "^8.0",
        "stubbles/reflect": "^8.0.2",
        "stubbles/date": "^7.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.7",
        "phpunit/phpunit": "^8.4",
        "bovigo/callmap": "^5.0",
        "bovigo/assert": "^5.0",
        "stubbles/sequence": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\xml\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\xml\\": "src/test/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
