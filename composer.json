{
    "name": "stubbles/xml",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Classes to work with XML",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "stubbles/ioc": "^11.0",
        "stubbles/reflect": "^10.0",
        "stubbles/date": "^9.0"
    },
    "require-dev": {
        "ext-xsl": "*",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "mikey179/vfsstream": "^1.6.11",
        "phpunit/phpunit": "^10.5",
        "bovigo/callmap": "^8.0",
        "bovigo/assert": "^8.0",
        "stubbles/sequence": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\xml\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stubbles\\xml\\": "src/test/php",
            "stubbles\\helper\\": "src/test/helper"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
