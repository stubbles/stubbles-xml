{
    "name": "stubbles/xml",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Classes to work with XML",
    "require": {
        "php": "^7.0",
        "stubbles/ioc": "^7.1",
        "stubbles/reflect": "^7.0",
        "stubbles/date": "^6.0"
    },
    "require-dev": {
